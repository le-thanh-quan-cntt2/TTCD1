@model IEnumerable<web_bán_giày_ttcd.Models.QUAN_TRI>

@{
    ViewBag.Title = "Index";
}
<link href="~/Content/CssQuanTri/Style.css" rel="stylesheet" />

<h2>Index</h2>

<p>
    @Html.ActionLink("Them moi", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Mat_khau)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Trang_thai)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Mat_khau)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Trang_thai)
            </td>
            <td>
                @Html.ActionLink("Sua", "Edit", new { id = item.Tai_khoan }) |
                @Html.ActionLink("Chi tiet", "Details", new { id = item.Tai_khoan }) |
                @Html.ActionLink("Xoa", "Delete", new { id = item.Tai_khoan })
            </td>
        </tr>
    }

</table>
